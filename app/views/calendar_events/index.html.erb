<p id="notice"><%= notice %></p>

<h1>Calendar Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Event contact</th>
      <th>Event start</th>
      <th>Event end</th>
      <th>All day</th>
      <th>Status</th>
      <th>Profile</th>
      <th>Group</th>
      <th>Group visibility</th>
      <th>Connection visibility</th>
      <th>User visibility</th>
      <th>Public visibility</th>
      <th>Rating</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @calendar_events.each do |calendar_event| %>
      <tr>
        <td><%= calendar_event.name %></td>
        <td><%= calendar_event.description %></td>
        <td><%= calendar_event.event_contact %></td>
        <td><%= calendar_event.event_start %></td>
        <td><%= calendar_event.event_end %></td>
        <td><%= calendar_event.all_day %></td>
        <td><%= calendar_event.status %></td>
        <td><%= calendar_event.profile %></td>
        <td><%= calendar_event.group %></td>
        <td><%= calendar_event.group_visibility %></td>
        <td><%= calendar_event.connection_visibility %></td>
        <td><%= calendar_event.users_visibility %></td>
        <td><%= calendar_event.public_visibility %></td>
        <td><%= calendar_event.rating %></td>
        <td><%= link_to 'Show', calendar_event %></td>
        <td><%= link_to 'Edit', edit_calendar_event_path(calendar_event) %></td>
        <td><%= link_to 'Destroy', calendar_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Calendar Event', new_calendar_event_path %>
